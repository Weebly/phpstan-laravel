{
    "name": "weebly/phpstan-laravel",
    "license": "BSD-2-Clause",
    "description": "Laravel plugins for PHPStan",
    "type": "library",
    "authors": [
        {
            "name": "Chris Leppanen",
            "email": "chris.leppanen@weebly.com"
        }
    ],
    "require": {
        "illuminate/auth": "5.5.* || 5.6.*",
        "illuminate/broadcasting": "5.5.* || 5.6.*",
        "illuminate/database": "5.5.* || 5.6.*",
        "illuminate/http": "5.5.* || 5.6.*",
        "illuminate/view": "5.5.* || 5.6.*",
        "phpstan/phpstan": "^0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.2"
    },
    "autoload": {
        "psr-4": {
            "Weebly\\PHPStan\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Weebly\\PHPStan\\Laravel\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
